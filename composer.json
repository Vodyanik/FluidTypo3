{
	"repositories": [
		{ "type": "vcs", "url": "https://github.com/mehrwert/TYPO3-phpMyAdmin.git" },
        { "type": "composer", "url": "https://composer.typo3.org/" },
        { "type": "path", "url": "packages/*/" }
